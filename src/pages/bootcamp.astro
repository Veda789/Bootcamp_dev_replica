---
import Layout from '../layouts/Layout2.astro';
import Header from '../components/header.astro';
import Content from '../components/test.astro';
import Content2 from '../components/middle.astro';
import File1 from '../pages/boot_p1.md';
import File2 from '../pages/boot_p2.md';
import File3 from '../pages/boot_p3.md';

---
<Layout title="bootcamp">
	<main>
		<Header/>
		<div class="d2">
            <File1/>
	    </div>
		<h2>Signups closed</h2>
		<div class="d3">
            <File2/>
	    </div>
		<div class="d2">
		    <h2 style="text-align:center;">THE SCHEDULE</h2>
			<p style="text-align:center;">Each week we'll unlock a new module</p>
            <File3/>
	    </div>
		<div class="d4">
		    <Content2/>
		</div>

		

	</main>
</Layout>
